{
    "name": "serendipity_hq/component-stopwatch",
    "type": "library",
    "description": "Measure your scripts (forked and improved from the Symofny's one)",
    "keywords": [],
    "homepage": "https://github.com/Aerendir/component-stopwatch",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Adamo Crespi",
            "email": "hello@aerendir.me"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "thecodingmachine/safe": "^0.1|^1.0"
    },
    "require-dev": {
        "ext-ast": "*",
        "drenso/phan-extensions": "@stable",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phan/phan": "^2",
        "phpstan/phpstan": "^0.11",
        "rector/rector": "^0.4.12",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^5",
        "thecodingmachine/phpstan-safe-rule": "^0.1.3",
        "vimeo/psalm": "^3.3",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Component\\Stopwatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Component\\Stopwatch\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "nikic/php-parser": "4.6.0"
    }
}
